<?xml version="1.0" encoding="UTF-8"?>
<odoo>

	<template id="theme_nextbiz_header9" inherit_id="website.layout" name="Header Nine" active="False">
		<xpath expr="//header//nav" position="replace">
			<!-- <t t-call="theme_nextbiz.search_popup"/> -->
			<nav data-name="Navbar" id="vouge-header9" t-attf-class="vouge-header9 navbar-light">
				<section class="top-header d-none d-md-block">
					<div class="container">
						<div class="row align-items-center">
							<div class="col-md-7">
								<div class="contact-time d-flex">
									<p class="text-primary mb-0"><span class="fa fa-phone text-primary me-2"/><span t-out="res_company.phone"/></p>
									<p class="text-primary mb-0 ms-4"><span class="fa fa-clock text-primary me-2"/><span>Mon-Fri : 9:00 - 18:00</span></p>
								</div>
							</div>
							<div class="col-md-5">
								<div class="header-media text-end">
									<a class="link text-primary me-3" t-att-href="website.social_facebook"
										t-if="website.social_facebook" target="_blank">
										<i class="fa fa-facebook" />
									</a>
									<a class="link text-primary me-3" t-att-href="website.social_twitter"
										t-if="website.social_twitter" target="_blank">
										<i class="fa fa-twitter" />
									</a>
									<a class="link text-primary me-3" t-att-href="website.social_linkedin"
										t-if="website.social_linkedin" target="_blank">
										<i class="fa fa-linkedin" />
									</a>
									<a class="link text-primary me-3" t-att-href="website.social_youtube"
										t-if="website.social_youtube" target="_blank">
										<i class="fa fa-youtube-play" />
									</a>
									<a class="link text-primary me-3" t-att-href="website.social_instagram"
										t-if="website.social_instagram" target="_blank">
										<i class="fa fa-instagram"></i>
									</a>
									<a class="link text-primary me-3" t-att-href="website.social_github"
										t-if="website.social_github" target="_blank">
										<i class="fa fa-github"></i>
									</a>
									
									<div class="login_nav ms-1">
										<ul class="list-unstyled m-0">
											<t t-call="portal.placeholder_user_sign_in">
												<t t-set="_item_class" t-valuef="nav-item ms-lg-auto"/>
												<t t-set="_link_class" t-valuef="nav-link fw-bold"/>
											</t>
											<!-- User Dropdown -->
											<t t-call="portal.user_dropdown">
							                	<t t-set="_icon" t-value="true"/>

												<t t-set="_user_name" t-value="false"/>
												<t t-set="_item_class" t-valuef="nav-item dropdown ms-lg-auto"/>
												<t t-set="_link_class" t-valuef="nav-link fw-bold"/>
											</t>
											
										</ul>
									</div>
								</div>
							</div>
						</div>
					</div>
				</section>
				<div class="container middle-container">
					<section class="middle-header">
						<div class="row align-items-center">
							<div class="col-md-2 d-flex d-md-block mbl-div header_left">
								<div class="vouge-logo text-start">
									<a href="/" class="navbar-brand logo m-0">
							            <span t-field="website.logo" t-options="{'widget': 'image'}" role="img" t-att-aria-label="'Logo of %s' % website.name" t-att-title="website.name" />
							        </a>
								</div>
								<div class="mobile-sidebar-toggle d-block d-md-none">
									<button type="button" id="show-sidebar" class="btn btn-sm btn-dark d-block d-md-none">
										<span class="ti ti-view-grid text-secondary fa-2x"></span>
									</button>
								</div>
							</div>
							<div class="col-md-8 d-none d-md-block header-center">
								<nav class="navbar navbar-expand-md p-0" id="bizople-header-nine">
									<div class="collapse navbar-collapse" id="header9-menuitem">
										<!-- <ul class="nav navbar-nav" id="top_menu"> -->
										<t t-call="website.navbar_nav" id="top_menu">
                        					<t t-set="_nav_class" t-valuef="me-auto"/>
											<t t-foreach="website.menu_id.child_id" t-as="submenu">
												<t t-call="website.submenu">
													<t t-set="item_class" t-value="'nav-item'" />
													<t t-set="link_class" t-value="'nav-link'" />
												</t>
											</t>
										</t>
										<!-- </ul> -->
										<ul class="list-unstyled m-0">
											<t t-call="website.placeholder_header_language_selector">
											<t t-set="_div_classes" t-value="''"/>
										</t>
										</ul>
									</div>
								</nav>
							</div>
							<div class="col-md-2 d-none d-md-block header-center">
								<div class="s_btn text-end" data-name="Button">
    								<a href="#" class="btn btn-primary rounded-circle btn-lg">Enquiry</a>
        						</div>
							</div>
						</div>
					</section>
				</div>
				<!-- <t t-call="theme_nextbiz.bizople_mbl_side_bar"/> -->
			</nav>
		</xpath>
		<xpath expr="//header" position="after">
			<div class="d-block d-md-none">
				<t t-call="theme_nextbiz.bizople_mobile_bottom_icon_bar"/>
			</div>
		</xpath>
	</template>

	

	<template id="template_header_five_align_center" inherit_id="theme_nextbiz.theme_nextbiz_header9" active="False">
        <xpath expr="//ul[@id='top_menu']" position="attributes">
            <attribute name="class">nav navbar-nav mx-md-auto</attribute>
        </xpath>
    </template>

    <template id="template_header_five_align_right" inherit_id="theme_nextbiz.theme_nextbiz_header9" active="False">
        <xpath expr="//ul[@id='top_menu']" position="attributes">
            <attribute name="class">nav navbar-nav ms-md-auto</attribute>
        </xpath>
    </template>
</odoo>